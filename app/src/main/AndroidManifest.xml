<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="dyly.bloomu.edu.gardenplannerapp" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/garden_planner_pro_icon"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".BUOCActivity"
            android:label="BUOC"/>

        <activity android:name=".AboutAppActivity"
            android:label="About Application"/>

        <activity android:name=".SupportActivity"
            android:label="Support"/>
    </application>

</manifest>
